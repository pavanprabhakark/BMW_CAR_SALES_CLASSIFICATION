{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SRC_DIM_BMW_MODEL",
						"type": "DatasetReference"
					},
					"name": "SRCDIMBMWCARMODELS"
				},
				{
					"dataset": {
						"referenceName": "DS_SRC_TMP_BMW_CAR_MODELS",
						"type": "DatasetReference"
					},
					"name": "SRCDIMTMPCARMODELS"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "InnerJoin"
				}
			],
			"scriptLines": [
				"source(output(",
				"          BMW_ID as long,",
				"          Model as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCDIMBMWCARMODELS",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCDIMTMPCARMODELS",
				"SRCDIMBMWCARMODELS join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> InnerJoin"
			]
		}
	}
}